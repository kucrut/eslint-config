{
	"name": "@kucrut/eslint-config",
	"version": "0.4.0",
	"author": "Dzikri Aziz",
	"description": "ESLint configuration for JS & Svelte files.",
	"license": "MIT",
	"packageManager": "pnpm@10.29.3",
	"type": "module",
	"types": "types/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./types/index.d.ts",
			"import": "./configs/index.js"
		}
	},
	"files": [
		"configs",
		"types"
	],
	"keywords": [
		"eslint",
		"eslintconfig",
		"eslint-config"
	],
	"scripts": {
		"generate-types": "node scripts/generate-types.js"
	},
	"dependencies": {
		"@eslint/js": "^9.39.2",
		"@stylistic/eslint-plugin": "^5.8.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.28.1",
		"@eslint/js": "^9.39.2",
		"@stylistic/eslint-plugin": "^5.8.0",
		"@types/eslint": "^9.6.1",
		"dts-buddy": "^0.6.2",
		"eslint": "^9.39.2",
		"eslint-plugin-svelte": "^3.0.3",
		"svelte": "^5.22.4",
		"svelte-eslint-parser": "^1.4.0"
	},
	"peerDependencies": {
		"eslint": "^9.39.2",
		"eslint-plugin-svelte": "^3.15.0",
		"svelte": "^4.2.19 || ^5.50.0",
		"svelte-eslint-parser": "^1.4.0"
	},
	"peerDependenciesMeta": {
		"eslint-plugin-svelte": {
			"optional": true
		},
		"svelte-eslint-parser": {
			"optional": true
		},
		"svelte": {
			"optional": true
		}
	}
}
