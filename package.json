{
	"name": "@kucrut/eslint-config",
	"version": "0.3.2",
	"author": "Dzikri Aziz",
	"description": "ESLint configuration for JS & Svelte files.",
	"license": "MIT",
	"packageManager": "pnpm@9.14.2",
	"type": "module",
	"types": "types/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./types/index.d.ts",
			"import": "./configs/index.js"
		}
	},
	"files": [
		"configs",
		"types"
	],
	"keywords": [
		"eslint",
		"eslintconfig",
		"eslint-config"
	],
	"scripts": {
		"generate-types": "node scripts/generate-types.js"
	},
	"dependencies": {
		"@eslint/js": "^9.16.0",
		"@stylistic/eslint-plugin": "^2.11.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.10",
		"@eslint/js": "^9.15.0",
		"@stylistic/eslint-plugin": "^2.11.0",
		"@types/eslint": "^9.6.1",
		"@types/eslint__js": "^8.42.3",
		"dts-buddy": "^0.5.3",
		"eslint": "^9.15.0",
		"eslint-plugin-svelte": "^2.46.0",
		"svelte": "^5.2.7",
		"svelte-eslint-parser": "^0.43.0"
	},
	"peerDependencies": {
		"eslint": ">= 9.15.0",
		"eslint-plugin-svelte": "^2.46.0",
		"svelte": "^4.2.19 || ^5.2.7",
		"svelte-eslint-parser": "^0.43.0"
	},
	"peerDependenciesMeta": {
		"eslint-plugin-svelte": {
			"optional": true
		},
		"svelte-eslint-parser": {
			"optional": true
		},
		"svelte": {
			"optional": true
		}
	}
}
